<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CoreReactReduxTypeScript</title>
    <base href="~/" />

    @foreach (var stylesheetSrc in StartUpVendors.CssVendors)
    {
        <link rel="stylesheet" href="@stylesheetSrc" asp-append-version="true" />
    }
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: true)
</body>
</html>
