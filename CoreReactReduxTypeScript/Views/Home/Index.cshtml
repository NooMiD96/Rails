@{
    ViewData["Title"] = "Home Page";
}

<div id="react-app"
     asp-prerender-module=@($"{StartUpVendors.ServerPhysicalPath}/{StartUpVendors.ServerFileName}")
     asp-prerender-data="new {
        user = @ViewData["user"]
     }">Loading...</div>

@section scripts {
    @foreach (var scriptSrc in StartUpVendors.JsVendors)
    {
        <script src="@scriptSrc" asp-append-version="true"></script>
    }
}
